<body>
<!-- sign in -->
<div class="sign section--full-bg" data-bg="{{ asset('front/img/bg.jpg') }}">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <div class="sign__content">
                    <!-- authorization form -->


        {{ form_start(registrationForm, {'attr': {class:'sign__form'}}) }}
                    <a href="{{ path('app_index') }}" class="sign__logo">
                        <img src="{{ asset('front/img/logo.svg') }}" alt="">
                    </a>
                    <h1 class="footer__tagline">
                        Verify your email after register.
                    </h1>

                    <div class="sign__group">

                        {{ form_widget(registrationForm.name,  {'attr': {'class': 'sign__input', type:'text' , placeholder:'Name' }}) }}
                        <h1 class="footer__tagline">
                            {{ form_errors(registrationForm.name) }}
                        </h1>

                    </div>
                    <div class="sign__group">
                        {{ form_widget(registrationForm.email,  {'attr': {'class': 'sign__input', type:'email' , autocomplete: 'email', placeholder:'Email' }}) }}
                        <h1 class="footer__tagline">
                            {{ form_errors(registrationForm.email) }}
                        </h1>

                    </div>
                        <div class="sign__group">

                            {{ form_widget(registrationForm.plainPassword,  {'attr': {'class': 'sign__input', placeholder:'Password'}})  }}
                            <h1 class="footer__tagline">
                                {{ form_errors(registrationForm.plainPassword) }}
                            </h1>
                        </div>
                        <div class="sign__group sign__group--checkbox">
                            {{ form_widget(registrationForm.agreeTerms,{ 'attr':{class : 'sign__group sign__group--checkbox',type: 'checkbox', checked :'checked'}}) }}
                        </div>

                    <div class="sign__group">

                        {{ form_widget(registrationForm.bday,  {'attr': {'class': 'sign__date', type:'date'}})  }}
                        <h1 class="footer__tagline">
                            {{ form_errors(registrationForm.bday) }}
                        </h1>
                    </div>

                    <div class="col-12">
                        <div class="form__gallery">
                        <label id="gallery1" for="form__gallery-upload">Upload photos</label>
                        {{ form_widget(registrationForm.image,  {'attr': {'class': 'form__gallery-upload', type:'file'}})  }}
                        <h1 class="footer__tagline">
                            {{ form_errors(registrationForm.image) }}
                        </h1>
                        </div>
                    </div>

                            <button type="submit" class="sign__btn">Register</button>
                    {{ form_end(registrationForm) }}
                    <!-- end authorization form -->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end sign in -->

