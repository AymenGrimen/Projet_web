<body>
<!-- sign in -->
<div class="sign section--full-bg" data-bg="{{ asset('front/img/bg.jpg') }}">
    <div class="container">
        <div class="row">
            <h1 class="col-12">
                <h1 class="sign__content">
                    <!-- authorization form -->
                    <form method="post" class="sign__form">

                        <a href="{{ path('app_index') }}" class="sign__logo">
                            <img src="{{ asset('front/img/logo.svg') }}" alt="">
                        </a>
                        {% if error %}
                        <h1 class="footer__tagline">
                            {{ error.messageKey|trans(error.messageData, 'security') }}
                        </h1>
                        {% endif %}
                        {% for message in app.flashes('success') %}
                            <h1 class="footer__tagline">
                                {{ message }}
                            </h1>
                        {% endfor %}


                        <div class="sign__group">
                            <input type="email" value="{{ last_username }}" class="sign__input" name="email" id="inputEmail" autocomplete="email" required autofocus placeholder="Email">
                        </div>

                        <div class="sign__group">
                            <input type="password" name="password" id="inputPassword" class="sign__input" autocomplete="current-password" required placeholder="Password">

                        </div>
                        <input type="hidden" name="_csrf_token"
                               value="{{ csrf_token('authenticate') }}"
                        >

                        <div class="sign__group sign__group--checkbox">
                            <input id="remember" name="remember" type="checkbox" checked="checked">
                            <label for="remember">Remember Me</label>
                        </div>

                        <button class="sign__btn" type="submit">Sign in</button>

                        <span class="sign__text">Don't have an account? <a href="{{ path('app_register') }}">Sign up!</a></span>

                        <span class="sign__text"><a href="{{ path('reset_password') }}">Forgot password?</a></span>
                    </form>
                    <!-- end authorization form -->
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end sign in -->

